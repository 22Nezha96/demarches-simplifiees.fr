#backoffice


  %table.table
    %thead
      %th Procédure
      %th Dossier
      %th etat
      %th Date de mise à jour
    - @dossiers.each do |dossier|
      %tr
        %td= dossier.procedure.libelle
        %td
          = link_to(dossier.nom_projet, "/backoffice/dossiers/#{dossier.id}")
        %td Mise à jour
        %td= dossier.last_update